}

button {
  padding: 10px 20px;
  margin: 0 10px;
  border: 2px solid rgb(26, 65, 81);
  border-radius: 5px;
  background-color: #eeeeee;
  color: #fff;
  cursor: pointer;
  transition: all 0.3s ease;
  outline: none;
}

button:hover {
  background-color: rgb(26, 65, 81);
  border: 2px solid white;
}

button img {
  width: 20px;
  height: 20px;
  margin-right: 5px;
  vertical-align: middle;
}

@keyframes buttonSlide {
  0% {
    transform: translateY(-20px);
    opacity: 0;
  }
  100% {
    transform: translateY(0);
    opacity: 1;
  }
}

    </style>
